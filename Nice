class country:
    def __init__ (self, name, population, economy, military):
        self.name=name
        self.population=population
        self.economy=economy
        self.military=military
    def getName(self):
        return self.name
    def setName(self, newName):
        self.name=newName
    def getEconomy(self):
        return self.economy
    def setName(self, newEconomy):
        self.name=newEconomy
    def __str__(self):
        return ("no")
myName=input("choose your country's name!")
myPopulation=75000000
print ("your population is 75,000,000")
myEcononmy=input("choose how your country's economy will look (in percents, which add to 100%")
myMilitary=input("choose your country's military power in percentages, which add to 100% (like units that you will have)")
myCountry=country(myName, myPopulation, myEcononmy, myMilitary)
print (myCountry)
newName=input("choose a new name")
myCountry.setName(newName)
