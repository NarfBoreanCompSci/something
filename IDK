class person:
    legs=2
    arms=2
    def __init__ (self, hairColor, name):
        self.hairColor=hairColor
        self.name=name
    def getHairColor(self):
        return self.hairColor
    def setHairColor(self, newHairColor):
        self.hairColor=newHairColor
    def toString(self):
        return "my name is "+(self.name)+" I have "+str( self.legs)+" legs, and "+ str(self.arms)+" arms"


timmy=person("orange", "timmy")
print (timmy.toString())
class person:
    legs=2
    arms=2
    def __init__ (self, hairColor, name):
        self.hairColor=hairColor
        self.name=name
    def getHairColor(self):
        return self.hairColor
    def setHairColor(self, newHairColor):
        self.hairColor=newHairColor
    def __str__(self):
        return "my name is "+(self.name)+" I have "+str( self.legs)+" legs, and "+ str(self.arms)+" arms"


timmy=person("orange", "timmy")
print (str(timmy))

class card:
    def __init__ (self, suit, number):
        self.suit=suit
        self.number=number
    def setSuit(self,suit):
        self.suit=suit
    def setNumber(self,number):
        self.number=number
    def getSuit(self):
        return self.suit
    def getNumber(self):
        return self.number
    def __str__(self):
        if self.number==13:
            return "king of "+ self.suit
        elif self.number==12:
            return "queen of "+ self.suit
        elif self.number==11:
            return "jack of "+ self.suit
        elif self.number==1:
            return "ace of "+ self.suit
        else:
            return (str(self.number) +"of"+ self.suit)
            
card1=card("spades", 10)
print (str(card1))
    
        
        
