class person:
    legs=2
    arms=2
    def __init__ (self, hairColor, name):
        self.hairColor=hairColor
        self.name=name
    def getHairColor(self):
        return self.hairColor
    def setHairColor(self, newHairColor):
        self.hairColor=newHairColor
    def toString(self):
        return "my name is "+(self.name)+" I have "+str( self.legs)+" legs, and "+ str(self.arms)+" arms"


timmy=person("orange", "timmy")
print (timmy.toString())
class person:
    legs=2
    arms=2
    def __init__ (self, hairColor, name):
        self.hairColor=hairColor
        self.name=name
    def getHairColor(self):
        return self.hairColor
    def setHairColor(self, newHairColor):
        self.hairColor=newHairColor
    def __str__(self):
        return "my name is "+(self.name)+" I have "+str( self.legs)+" legs, and "+ str(self.arms)+" arms"


timmy=person("orange", "timmy")
print (str(timmy))
        
        
